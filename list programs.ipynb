    "def factorial(i):\n",
    "    fact=1\n",
    "    for j in range(1,i+1):\n",
    "        fact*=j\n",
    "    return fact\n",
    "print(\"enter 5 integers\")\n",
    "a=[int(input()) for i in range(5)]\n",
    "f=[factorial(i) for i in a]\n",
    "print(\"the given list is \",a)\n",
    "print(\"factorial of the list is \",f)"
   ]
